{
  "name": "webpack-react-npm",
  "version": "1.0.0",
  "description": "Babel and React, bundled with Webpack for creating NPM modules",
  "main": "src/js/app.js",
  "module": "src/dist/js/bundle.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-merge": "^4.1.1"
  },
  "scripts": {
    "start": "start http://localhost:3000/ && webpack-dev-server --config webpack/dev.config.js",
    "build:npm": "babel src/js/app.js --out-file ./lib/bundle.js --source-maps inline --presets=react && node-sass ./src/scss/index.scss ./lib/css/index.css",
    "build:webpack": "webpack --config webpack/prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Demosth3nes/webpack-react-npm.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Demosth3nes/webpack-react-npm/issues"
  },
  "homepage": "https://github.com/Demosth3nes/webpack-react-npm#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
